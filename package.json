{
  "name": "hands-on",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-debug": "DEBUG=hands-on:* ./bin/www",
    "build": "./node_modules/.bin/browserify -t babelify -t bulkify browser.js public/bundle.js",
    "watch": "./node_modules/.bin/watchify -d -v -t babelify -t bulkify browser.js -o public/bundle.js & ./node_modules/.bin/browser-sync start --proxy 'localhost:3000' --files public/**/*"
  },
  "dependencies": {
    "body-parser": "~1.12.0",
    "browser-sync": "^2.4.0",
    "classnames": "^1.2.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "morgan": "~1.5.1",
    "react": "^0.13.1",
    "react-bootstrap": "^0.19.1",
    "react-router": "^0.13.2",
    "react-router-bootstrap": "^0.12.1"
  },
  "devDependencies": {
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "bulkify": "^1.1.1",
    "watchify": "^2.6.2"
  }
}
